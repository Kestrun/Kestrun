            "console": "integratedTerminal"
        },
        {
            "name": "Launch pwsh with script",
            "type": "coreclr",
            "request": "launch",
            "program": "pwsh",
            "args": [
                "-NoLogo",
                "-NoProfile",
                "-ExecutionPolicy",
                "Bypass",
                "-File",
                "${file}"
            ],
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "console": "integratedTerminal"
        },
        {
            "name": "Launch pwsh with script (bootstrap)",
            "type": "coreclr",
            "request": "launch",
            "program": "pwsh",
            "args": [
                "-NoLogo",
                "-NoProfile",
                "-ExecutionPolicy",
                "Bypass",
                "-Command",
                "  import-module '.\\src\\PowerShell\\Kestrun\\Kestrun.psm1'; . '${file}'  "
            ],
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "console": "integratedTerminal"
        },
        {
            "name": "PowerShell Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "cwd": "${cwd}"
        }
    ]
