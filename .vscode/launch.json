{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell: Launch MultiRoutes.ps1",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/examples/PowerShell/MultiRoutes.ps1",
            "cwd": "${workspaceFolder}/examples/PowerShell"
        },
        {
            "name": "PowerShell: Launch GlobalVar.ps1",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/examples/PowerShell/GlobalVar.ps1",
            "cwd": "${workspaceFolder}/examples/PowerShell"
        },
        {
            "name": ".NET Launch MultiRoutes",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build MultiRoutes",
            "program": "${workspaceFolder}/examples/CSharp/MultiRoutes/bin/Debug/net8.0/MultiRoutes.dll",
            "args": [],
            "cwd": "${workspaceFolder}/examples/CSharp/MultiRoutes",
            "stopAtEntry": false,
            "console": "integratedTerminal"
        },
        {
            "name": ".NET Launch CSharp/Certificates",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Certificates",
            "program": "${workspaceFolder}/examples/CSharp/Certificates/bin/Debug/net8.0/Certificates.dll",
            "args": [],
            "cwd": "${workspaceFolder}/examples/CSharp/Certificates",
            "stopAtEntry": false,
            "console": "integratedTerminal"
        },
        {
            "name": ".NET Launch CSharp/GlobalVar",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build GlobalVar",
            "program": "${workspaceFolder}/examples/CSharp/GlobalVar/bin/Debug/net8.0/GlobalVar.dll",
            "args": [],
            "cwd": "${workspaceFolder}/examples/CSharp/GlobalVar",
            "stopAtEntry": false,
            "console": "integratedTerminal"
        },
        {
            "name": ".NET Launch CSharp/RazorSample",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build RazorSample",
            "program": "${workspaceFolder}/examples/CSharp/RazorSample/bin/Debug/net8.0/RazorSample.dll",
            "args": [],
            "cwd": "${workspaceFolder}/examples/CSharp/RazorSample",
            "stopAtEntry": false,
            "console": "integratedTerminal"
        }
    ]
}