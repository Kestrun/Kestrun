{
    "version": "0.2.0",
    "configurations": [
        
         {
            "name": "PowerShell: Launch test.ps1",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/examples/PowerShell/test.ps1",
            "cwd": "${workspaceFolder}/examples/PowerShell"
        },
        {
            "name": ".NET Launch CSharpTest",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build CSharpTest",
            "program": "${workspaceFolder}/examples/CSharpTest/bin/Debug/net8.0/CSharpTest.dll",
            "args": [],
            "cwd": "${workspaceFolder}/examples/CSharpTest",
            "stopAtEntry": false,
            "console": "integratedTerminal"
        }
    ]
}